        "libphonenumber-js": "^1.9.47",
        "mime": "^3.0.0",
        "minimist": "^1.2.5",
        "moment": "^2.29.1",
        "nodemailer": "^6.7.2",
        "nodemon": "^2.0.15",
        "npm": "^8.4.0",
        "nyc": "^15.1.0",
        "passport": "^0.5.2",
        "passport-custom": "^1.1.1",
