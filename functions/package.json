{
    "name": "amplywealth-api",
    "version": "1.0.0",
    "description": "amplywealth-api",
    "main": "index.js",
    "directories": {
        "test": "test"
    },
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "test": "NODE_ENV=test env-cmd -f ./env/.env.test ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "autoTest": "NODE_ENV=test  env-cmd -f ./env/.env.test ./node_modules/.bin/supervisor -q -n exit -x ./node_modules/.bin/mocha -- --recursive --config test/.mocharc.js",
        "devServer": "env-cmd -f ./env/.env.dev nodemon  index.js",
        "mockTrueData": "env-cmd -f ./env/.env.dev nodemon mock-truedata.js",
        "buildEnv": "node  build-env-index.js",
        "coverage": "NODE_ENV=test env-cmd -f ./env/.env.test nyc --reporter=html ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "coverageBitBucket": "NODE_ENV=test env-cmd -f ./env/.env.test nyc --reporter=lcov ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "stockStreamMock": "env-cmd -f ./env/.env.dev nodemon stock-stream-server.js",
        "stockStream": "env-cmd -f ./env/.env.dev nodemon stock-stream-server.js REALTIME"
    },
    "author": "",
    "license": "",
